            <DataRight>0</DataRight>
            <DataBottom>0</DataBottom>
            <Source><![CDATA[&Tp1cod1  = 4201
&Tp1corr1 = 5
&Tp1corr2 = 1
&Tp1corr3 = 0 
For Each Tp1cod, Tp1cod1, Tp1corr1, Tp1corr2, Tp1corr3 // Fst198: Fst198
Where Tp1cod   = &Pgcod
Where Tp1cod1  = &Tp1cod1
Where Tp1corr1 = &Tp1corr1
Where Tp1corr2 = &Tp1corr2
Where Tp1corr3 = &Tp1corr3
	&MSGA00Id  = Tp1nro1
EndFor

For Each // JEFN003: JEFN003
Where JEFN003NST = &nrosoli
Defined By JEFN003NU2
	&JEFN003RCM = JEFN003RCM
EndFor

If &Mode = 'INS'
    //Cargo el cuerpo con los datos correspondientes
    &MSGA50BODY = '<p>' + 'Se ingresó una solicitud: ' +'<br/>'+'Número de Solicitud: '+&nrosoli.ToString()
                +'<br/>'+'Nombre del usuario: '+&usuario+'<br/>'+'Fecha de ingreso: '+&Pgfape.ToString()+'<p>'
        
    &MSGA50Sb   = 'Nuevo ingreso de Solicitud Talonario' //&aAsunto
    &MSGA50Rl   = 0

Else
    If &Mode = 'UPD'
        //Cargo el cuerpo con los datos correspondientes
        &MSGA50BODY = '<p>' + 'Se canceló una solicitud: ' +'<br/>'+'Número de Solicitud: '+&nrosoli.ToString()
                    +'<br/>'+'Usuario que canceló: '+&usuario+'<br/>'+'Fecha de cancelación: '+&Pgfape.ToString()+'<p>'
            
        &MSGA50Sb   = 'Cancelación de Solicitud Talonario' //&aAsunto
        &MSGA50Rl   = 0

        
    Else
        If &Mode = 'ACP'
            &MSGA50BODY = '<p>' + 'Se confirmó una solicitud: ' +'<br/>'+'Número de Solicitud: '+&nrosoli.ToString()
                    +'<br/>'+'Usuario que confirmó: '+&usuario+'<br/>'+'Fecha de confirmación: '+&Pgfape.ToString()+'<p>'
            
            &MSGA50Sb   = 'Confirmación de Solicitud Talonario' //&aAsunto
            &MSGA50Rl   = 0

        Else
            If &Mode = 'APB'
                &MSGA50BODY = '<p>' + 'Se aprobó una solicitud: ' +'<br/>'+'Número de Solicitud: '+&nrosoli.ToString()
                    +'<br/>'+'Usuario que aprobó solicitud: '+&usuario+'<br/>'+'Fecha de aprobación: '+&Pgfape.ToString()+'<p>'  
                &MSGA50Sb   = 'Aprobación de Solicitud Talonario' //&aAsunto
                &MSGA50Rl   = 0
  
            Else
                If &Mode = 'INP'
                    &MSGA50BODY = '<p>' + 'Se ingresó la siguiente solicitud: '           +'<br/>' 
                    &MSGA50BODY = &MSGA50BODY + 'Número de Solicitud: '           +&nrosoli.ToString()    +'<br/>' 
                    &MSGA50BODY = &MSGA50BODY + 'Usuario que ingresó solicitud: ' +&usuario               +'<br/>' 
                    &MSGA50BODY = &MSGA50BODY + 'Fecha de ingreso: '              +&Pgfape.ToString()     +'<br/>' 
                    &MSGA50BODY = &MSGA50BODY + 'Pendiente de aprobación...'              +'<p>' 
                                       
                    &MSGA50Sb   = 'Nuevo Ingreso de Solicitud Talonario pendiente de aprobación' //&aAsunto
                    &MSGA50Rl   = 0 
                Else 
                    If &Mode = 'RCB'
                        &MSGA50BODY = '<p>' + 'Se recibió la siguiente solicitud: '           +'<br/>' 
                        &MSGA50BODY = &MSGA50BODY + 'Número de Solicitud: '           +&nrosoli.ToString()    +'<br/>' 
                        &MSGA50BODY = &MSGA50BODY + 'Usuario que recepcionó solicitud: ' +&usuario               +'<br/>' 
                        &MSGA50BODY = &MSGA50BODY + 'Fecha de recepción: '              +&Pgfape.ToString()     +'<p>'
                                        
                        &MSGA50Sb   = 'Recepción de Solicitud Talonario' //&aAsunto
                        &MSGA50Rl   = 0 
                    EndIf
                EndIf
            EndIf
        EndIf
    EndIf
EndIf

//perfiles
&atributo = 'EMAIL'
&Tp1cod1  = 4201
&Tp1corr1 = 1 
&Tp1corr2 = 3

&i = 1

If &JEFN003RCM = 200
    For Each Tp1cod, Tp1cod1, Tp1corr1, Tp1corr2, Tp1corr3 // Fst198: Guías especiales de proceso
    Where Tp1cod   = &Pgcod
    Where Tp1cod1  = &Tp1cod1
    Where Tp1corr1 = &Tp1corr1
    Where Tp1corr2 = &Tp1corr2
    Defined By Tp1nro1	
        If Tp1corr3 = 2
            &PERFILCO     = Trim(Tp1desc)
            &Perfiles(&i) = Trim(Tp1desc)
            &i += 1
        EndIf    
        If Tp1corr3 = 3
            &PERFILCO     = Trim(Tp1desc)
            &Perfiles(&i) = Trim(Tp1desc)
            &i += 1
        EndIf
        If Tp1corr3 = 4
            &PERFILCO     = Trim(Tp1desc)
            &Perfiles(&i) = Trim(Tp1desc)
            &i += 1
        EndIf
    EndFor
EndIf

If &JEFN003RCM = 100
    &Tp1corr2 = 4
    For Each Tp1cod, Tp1cod1, Tp1corr1, Tp1corr2, Tp1corr3 // Fst198: Guías especiales de proceso
    Where Tp1cod   = &Pgcod
    Where Tp1cod1  = &Tp1cod1
    Where Tp1corr1 = &Tp1corr1
    Where Tp1corr2 = &Tp1corr2
    Defined By Tp1nro1
        If Tp1corr3 = 1
            &PERFILCAJ  = Trim(Tp1desc)
            &Perfiles(&i) = Trim(Tp1desc)
            &i += 1
        EndIf
        
        If Tp1corr3 = 2
            &PERFILSUJ  = Trim(Tp1desc)
            &Perfiles(&i) = Trim(Tp1desc)
            &i += 1
        EndIf
        
        If Tp1corr3 = 3
            &PERFILSUJ  = Trim(Tp1desc)
            &Perfiles(&i) = Trim(Tp1desc)
            &i += 1
        EndIf
    EndFor
EndIf

&i          = 1
&Usuarios() = ''
&topeUs     = 0
Do While &Perfiles(&i) <> NullValue(&Perfiles(&i))
    For Each Pgcod, PrfGCod, Ubuser// PrfU00: Relac. Perfil/Usuarios
    Where Pgcod   = &Pgcod
    Where PrfGCod = &Perfiles(&i)
    Defined By PrfUTpo
        &user = Ubuser
        Do 'Agregar Usuario'      
    EndFor
    &i += 1
EndDo

&z = 1
Do While &z <= &topeUs
    &user = &Usuarios(&z)
    Do 'buscar usuario'  
    &z = &z + 1
EndDo

Sub 'buscar usuario'
     
 
     
    &PMSGA00  = 'PMSGA00'
    &Modo     = 'DSP'
    &ClaseMsg = &MSGA00Id
    Call(&PMSGA00,&ClaseMsg, &MSGA00Ds, &MSGA01Id, &MSGA00Rp, &MSGA00Rl, &MSGA00Pg, &MSGA00Ih, &MSGA00Fr, &MSGA00Sb, &Modo,&ErrId,&ErrDsc)
    &PSubject     = Trim(&MSGA50Sb)
    &PTextHTML    = Trim(&MSGA50BODY)
    &PFrom        = &MSGA00Fr
    &EstMCod      = 0 
    &PXWFMail     = 'PXWFMail'
    Call(&PXWFMail, &EstMCod,&PFrom, &Pextxt, &PCC, &PBCC, &PSubject, &PTextPlain, &PTextHTML, &PAttachments, &Error, &ErrSubject, &html)   
    
EndSub

Sub 'Agregar Usuario'
    &z      = 1
    &existe = 'N'
    Do While &z <= &topeUs
        If &user = &Usuarios(&z)
            &existe = 'S'
            Exit
        EndIf
        &z = &z + 1
    EndDo
    
    If &existe = 'N'
        &topeUs            = &topeUs + 1
        &Usuarios(&topeUs) = &user
    EndIf
EndSub // 'Agregar Usuario'
]]></Source>
